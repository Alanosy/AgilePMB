<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.org.alan.agile.mapper.TFilesMapper">
    <update id="updateProject">
        update T_Projects p
        set

        where p.id = #{itemId}

    </update>


    <select id="getFileList" resultType="cn.org.alan.agile.model.vo.project.FileListGetVo">

        SELECT *
        FROM T_Files fi
        WHERE fi.itemid =#{itemId}

    </select>
</mapper>
